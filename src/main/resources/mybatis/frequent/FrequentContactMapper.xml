<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bootdo.frequent.dao.FrequentContactDao">

    <select id="selectById" resultType="com.bootdo.frequent.domain.FrequentContact">
        select *
        from frequent_contact where id = #{value}
    </select>

    <select id="selectAll" resultType="com.bootdo.frequent.domain.FrequentContact">
        select *
        from  frequent_contact
    </select>

</mapper>